  geometry = new THREE.BoxGeometry(1, 1, 1);

  material = new THREE.MeshBasicMaterial({
    color: 0x00ff00,
    wireframe: true
  });

  cube = new THREE.Mesh(geometry, material);

  scene.add(cube);

  camera.position.z = 1;

  render = function() {
    requestAnimationFrame(render);
    cube.rotation.x += 0.1;
    cube.rotation.y += 0.1;
    return renderer.render(scene, camera);
  };

  render();

}).call(this);
